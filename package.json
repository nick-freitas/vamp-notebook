{
  "name": "vtm",
  "version": "0.0.1",
  "scripts": {
    "client:ng": "ng",
    "client:start": "ng serve",
    "client:build:prod": "ng build --prod",
    "client:build": "ng build",
    "client:test": "ng test",
    "client:lint": "ng lint",
    "client:e2e": "ng e2e",
    "server:build": "tsc -p ./src/server/tsconfig.build.json",
    "preserver:build": "rimraf dist/server",
    "server:format": "prettier --write \"src/server/app/**/*.ts\"",
    "server:start": "ts-node -r tsconfig-paths/register src/server/app/main.ts",
    "server:start:dev": "nodemon --config ./src/server/nodemon.json",
    "server:start:debug": "nodemon --config ./src/server/nodemon-debug.json",
    "preserver:start:prod": "rimraf dist/server && npm run server:build",
    "server:start:prod": "node dist/server/app/main.js",
    "dev": "concurrently \" ng serve \" \" npm run server:start:debug \" "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@ng-bootstrap/ng-bootstrap": "6.0.2",
    "@ngrx/data": "9.1.0",
    "@ngrx/effects": "8.6.0",
    "@ngrx/entity": "9.1.0",
    "@ngrx/store": "8.6.0",
    "@ngrx/store-devtools": "9.1.0",
    "angular-in-memory-web-api": "0.10.0",
    "bootstrap": "4.4.1",
    "bootstrap-icons": "1.0.0-alpha3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "core-js": "^2.5.4",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "pdffiller": "0.0.11",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.11.1",
    "@types/supertest": "^2.0.8",
    "codelyzer": "^5.1.2",
    "concurrently": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "~7.0.0",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.15.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/server",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 160
  },
  "resolutions": {
    "is-promise": "2.1.0"
  }
}
